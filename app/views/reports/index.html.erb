<h1>Hardcover books released in 2018</h1>


<table class="table">
  <thead>
    <tr>
      <th scope="col">Title</th>
      <th scope="col">Release Date</th>
    </tr>
  </thead>

  <tbody>
    <% @books.each do |book| %>
      <tr>
        <td><%= link_to book.title, book %></td>
        <td><%= book.release_date %></td>
      </tr>
    <% end %>
  </tbody>
</table>
